(declare-rel loop ((Array Int Int) Int Int Int Int ))
(declare-rel exit ())
(declare-var i Int )
(declare-var i1 Int )
(declare-var i2 Int )
(declare-var i3 Int )
(declare-var i4 Int )
(declare-var i5 Int )
(declare-var i6 Int )
(declare-var i7 Int )
(declare-var i8 Int )
(declare-var x Int )
(declare-var x1 Int )
(declare-var x2 Int )
(declare-var x3 Int )
(declare-var x4 Int )
(declare-var x5 Int )
(declare-var x6 Int )
(declare-var x7 Int )
(declare-var x8 Int )
(declare-var index Int )
(declare-var index1 Int )
(declare-var index2 Int )
(declare-var index3 Int )
(declare-var index4 Int )
(declare-var index5 Int )
(declare-var index6 Int )
(declare-var index7 Int )
(declare-var index8 Int )
(declare-var index_limit Int )
(declare-var a_i Int )
(declare-var a_i1 Int )
(declare-var a_i2 Int )
(declare-var a_i3 Int )
(declare-var a_i4 Int )
(declare-var a_i5 Int )
(declare-var a_i6 Int )
(declare-var a_i7 Int )
(declare-var count Int )
(declare-var a_array (Array Int Int) )

(rule (=> 
	(and 
		(= x (select a_array 0))
		(= index 0)
		(= i 0)
	)
	(loop a_array i count x index)
))
(rule (=> 
	(and 
		(loop a_array i count x index)
		(= index_limit (* count 8))
		(< i index_limit)
		
		(= a_i (select a_array i))
		(= x1 (ite (> a_i x) a_i x))
		(= index1 (ite (> a_i x) i index))
		(= i1 (+ i 1))

		(= a_i1 (select a_array i1))
		(= x2 (ite (> a_i1 x1) a_i1 x1))
		(= index2 (ite (> a_i1 x1) i1 index1))
		(= i2 (+ i1 1))

		(= a_i2 (select a_array i2))
		(= x3 (ite (> a_i2 x2) a_i2 x2))
		(= index3 (ite (> a_i2 x2) i2 index2))
		(= i3 (+ i2 1))	

		(= a_i3 (select a_array i3))
		(= x4 (ite (> a_i3 x3) a_i3 x3))
		(= index4 (ite (> a_i3 x3) i3 index3))
		(= i4 (+ i3 1))	

		(= a_i4 (select a_array i4))
		(= x5 (ite (> a_i4 x4) a_i4 x4))
		(= index5 (ite (> a_i4 x4) i4 index4))
		(= i5 (+ i4 1))

		(= a_i5 (select a_array i5))
		(= x6 (ite (> a_i5 x5) a_i5 x5))
		(= index6 (ite (> a_i5 x5) i5 index5))
		(= i6 (+ i5 1))

		(= a_i6 (select a_array i6))
		(= x7 (ite (> a_i6 x6) a_i6 x6))
		(= index7 (ite (> a_i6 x6) i6 index6))
		(= i7 (+ i6 1))

		(= a_i7 (select a_array i7))
		(= x8 (ite (> a_i7 x7) a_i7 x7))
		(= index8 (ite (> a_i7 x7) i7 index7))
		(= i8 (+ i7 1))
	)
	(loop a_array i8 count x8 index8)
))
(rule (=> 
	(and 
		(loop a_array i count x index)
		(not (< i index_limit))
	)
	exit
))
(query exit)