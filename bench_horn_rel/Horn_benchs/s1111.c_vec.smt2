(declare-rel loop ((Array Int Int) (Array Int Int) (Array Int Int) (Array Int Int) Int Int ))
(declare-rel exit ())
(declare-var i Int )
(declare-var i1 Int )
(declare-var i2 Int )
(declare-var i3 Int )
(declare-var i4 Int )
(declare-var t00 Int )
(declare-var t10 Int )
(declare-var t20 Int )
(declare-var t30 Int )
(declare-var t01 Int )
(declare-var t11 Int )
(declare-var t21 Int )
(declare-var t31 Int )
(declare-var t02 Int )
(declare-var t12 Int )
(declare-var t22 Int )
(declare-var t32 Int )
(declare-var t03 Int )
(declare-var t13 Int )
(declare-var t23 Int )
(declare-var t33 Int )
(declare-var index_limit Int )
(declare-var a_2i Int )
(declare-var a_2i1 Int )
(declare-var a_2i2 Int )
(declare-var a_2i3 Int )
(declare-var b_i Int )
(declare-var b_i1 Int )
(declare-var b_i2 Int )
(declare-var b_i3 Int )
(declare-var c_i Int )
(declare-var c_i1 Int )
(declare-var c_i2 Int )
(declare-var c_i3 Int )
(declare-var d_i Int )
(declare-var d_i1 Int )
(declare-var d_i2 Int )
(declare-var d_i3 Int )
(declare-var count Int )
(declare-var a_array (Array Int Int) )
(declare-var a_array1 (Array Int Int) )
(declare-var a_array2 (Array Int Int) )
(declare-var a_array3 (Array Int Int) )
(declare-var a_array4 (Array Int Int) )
(declare-var b_array (Array Int Int) )
(declare-var c_array (Array Int Int) )
(declare-var d_array (Array Int Int) )

(rule (=> 
	(and 
		(= i 0)
	)
	(loop a_array b_array c_array d_array i count)
))
(rule (=> 
	(and 
		(loop a_array b_array c_array d_array i count)
		
		(= index_limit (* count 4))
		(< i index_limit)
		
		(= b_i (select b_array i))
		(= c_i (select c_array i))
		(= d_i (select d_array i))
		(= t00 (* c_i b_i))
		(= t10 (* d_i b_i))
		(= t20 (* c_i c_i))
		(= t30 (* d_i c_i))
		(= a_2i (+ (+ (+ (+ t00 t10) t20) t10) t30))
		(= a_array1 (store a_array (* 2 i) a_2i))
		(= i1 (+ i 1))

		(= b_i1 (select b_array i1))
		(= c_i1 (select c_array i1))
		(= d_i1 (select d_array i1))
		(= t01 (* c_i1 b_i1))
		(= t11 (* d_i1 b_i1))
		(= t21 (* c_i1 c_i1))
		(= t31 (* d_i1 c_i1))
		(= a_2i1 (+ (+ (+ (+ t01 t11) t21) t11) t31))
		(= a_array2 (store a_array1 (* 2 i1) a_2i1))
		(= i2 (+ i1 1))

		(= b_i2 (select b_array i2))
		(= c_i2 (select c_array i2))
		(= d_i2 (select d_array i2))
		(= t02 (* c_i2 b_i2))
		(= t12 (* d_i2 b_i2))
		(= t22 (* c_i2 c_i2))
		(= t32 (* d_i2 c_i2))
		(= a_2i2 (+ (+ (+ (+ t02 t12) t22) t12) t32))
		(= a_array3 (store a_array2 (* 2 i2) a_2i2))
		(= i3 (+ i2 1))

		(= b_i3 (select b_array i3))
		(= c_i3 (select c_array i3))
		(= d_i3 (select d_array i3))
		(= t03 (* c_i3 b_i3))
		(= t13 (* d_i3 b_i3))
		(= t23 (* c_i3 c_i3))
		(= t33 (* d_i3 c_i3))
		(= a_2i3 (+ (+ (+ (+ t03 t13) t23) t13) t33))
		(= a_array4 (store a_array3 (* 2 i3) a_2i3))
		(= i4 (+ i3 1))
	)
	(loop a_array4 b_array c_array d_array i4 count)
))
(rule (=> 
	(and 
		(loop a_array b_array c_array d_array i count)
		(= index_limit (* count 4))
		(not (< i index_limit))
	)
	exit
))
(query exit)