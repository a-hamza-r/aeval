(declare-var a_array2 (Array Int Int) )
(declare-var a_array3 (Array Int Int) )
(declare-var a_array4 (Array Int Int) )
(declare-var a_array5 (Array Int Int) )
(declare-var a_array6 (Array Int Int) )
(declare-var a_array7 (Array Int Int) )
(declare-var a_array8 (Array Int Int) )
(declare-var b_array (Array Int Int) )

(rule (=> 
	(and 
		(= i 0)
	)
	(loop a_array b_array i count)
))
(rule (=> 
	(and 
		(loop a_array b_array i count)
		
		(= index_limit (* count 8))
		(< i index_limit)
		
		(= a_i (select a_array i))
		(= b_i (select b_array i))
		(= a_i1 (ite (> b_i 0) b_i a_i))
		(= a_array1 (store a_array i a_i1))
		(= i1 (+ i 1))

		(= a_i2 (select a_array1 i1))
		(= b_i1 (select b_array i1))
		(= a_i3 (ite (> b_i1 0) b_i1 a_i2))
		(= a_array2 (store a_array1 i1 a_i3))
		(= i2 (+ i1 1))

		(= a_i4 (select a_array2 i2))
		(= b_i2 (select b_array i2))
		(= a_i5 (ite (> b_i2 0) b_i2 a_i4))
		(= a_array3 (store a_array2 i2 a_i5))
		(= i3 (+ i2 1))

		(= a_i6 (select a_array3 i3))
		(= b_i3 (select b_array i3))
		(= a_i7 (ite (> b_i3 0) b_i3 a_i6))
		(= a_array4 (store a_array3 i3 a_i7))
		(= i4 (+ i3 1))

		(= a_i8 (select a_array4 i4))
		(= b_i4 (select b_array i4))
		(= a_i9 (ite (> b_i4 0) b_i4 a_i8))
		(= a_array5 (store a_array4 i4 a_i9))
		(= i5 (+ i4 1))

		(= a_i10 (select a_array5 i5))
		(= b_i5 (select b_array i5))
		(= a_i11 (ite (> b_i5 0) b_i5 a_i10))
		(= a_array6 (store a_array5 i5 a_i11))
		(= i6 (+ i5 1))

		(= a_i12 (select a_array6 i6))
		(= b_i6 (select b_array i6))
		(= a_i13 (ite (> b_i6 0) b_i6 a_i12))
		(= a_array7 (store a_array6 i6 a_i13))
		(= i7 (+ i6 1))

		(= a_i14 (select a_array7 i7))
		(= b_i7 (select b_array i7))
		(= a_i15 (ite (> b_i7 0) b_i7 a_i14))
		(= a_array8 (store a_array7 i7 a_i15))
		(= i8 (+ i7 1))
	)
	(loop a_array8 b_array i8 count)
))
(rule (=> 
	(and 
		(loop a_array b_array i count)
		(= index_limit (* count 8))
		(not (< i index_limit))
	)
	exit
))
(query exit)