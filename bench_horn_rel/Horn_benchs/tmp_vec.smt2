(declare-var b_array (Array Int Int) )

(rule (=> 
	(and 
		(= i 8)
		(= k 0)

		(< 1 (* count 8))
	)
	(loop a_array7 b_array i k count)
))
(rule (=> 
	(and 
		(loop a_array b_array i k count)
		(< i (* count 8))
	)
	(loop a_array8 b_array (+ i 8) k count)
))
(rule (=> 
	(and 
		(loop a_array b_array i k count)
		(not (< i (* count 8)))
	)
	exit
))
(query exit)
